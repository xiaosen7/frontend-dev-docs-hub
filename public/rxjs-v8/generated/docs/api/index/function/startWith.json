                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          \n      </td>\n    </tr>\n  </tbody>\n</table>\n\n\n<h4 class=\"no-anchor\" id=\"returns-3\">Returns</h4>\n<p><code><a href=\"api/index/interface/OperatorFunction\" class=\"code-anchor\">OperatorFunction</a>&#x3C;T, T | <a href=\"api/index/type-alias/ValueFromArray\" class=\"code-anchor\">ValueFromArray</a>&#x3C;A>></code></p>\n\n\n\n\n\n\n\n    </td>\n  </tr>\n  \n  </tbody></table>\n</section>\n\n\n  \n\n  \n<section class=\"see-also\">\n  <h2 id=\"see-also\">See Also<a title=\"Link to this heading\" class=\"header-link\" aria-hidden=\"true\" href=\"api/index/function/startWith#see-also\"><i class=\"material-icons\">link</i></a></h2>\n  <ul>\n  \n    <li><a href=\"api/index/function/endWith\"><code>endWith</code></a>\n</li>\n    <li><a href=\"api/index/function/finalize\"><code>finalize</code></a>\n</li>\n    <li><a href=\"api/index/function/concat\"><code>concat</code></a>\n</li>\n  </ul>\n</section>\n\n\n\n  </div>\n</article>\n\n<!-- links to this doc:\n - api/index\n - api/index/const/EMPTY\n - api/index/const/NEVER\n - api/index/function/combineLatest\n - api/index/function/concat\n - api/index/function/endWith\n - api/index/function/scan\n - api/operators/endWith\n - api/operators/scan\n - api/operators/startWith\n - deprecations/breaking-changes\n - deprecations/scheduler-argument\n - guide/scheduler\n-->\n<!-- links from this doc:\n - /api\n - api/index\n - api/index/function/concat\n - api/index/function/endWith\n - api/index/function/finalize\n - api/index/function/map\n - api/index/function/startWith#description\n - api/index/function/startWith#examples\n - api/index/function/startWith#overloads\n - api/index/function/startWith#see-also\n - api/index/function/startWith#startwith\n - api/index/function/timer\n - api/index/in